    "import pandas as pd\n",
    "import json\n",
    "filename = './city.json'\n",
    "with open(filename,'rb') as f_ojb:\n",
    "    json_data = json.load(f_ojb,)\n",
    "df=pd.read_json(json.dumps(json_data),orient='records')\n",
    "df.to_csv('test.csv',index=False,columns=[\"name\",\"log\",\"lat\"])    \n"
   ]
  }
