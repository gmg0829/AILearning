   "source": [
    "import json\n",
    "# 原始数据\n",
    "dict1 = {\n",
    "    'name': '翠花',\n",
    "    \"age\": 18,\n",
    "    'nickname': 'GreenFlower',\n",
    "}\n",
    "print(dict1)\n",
    " \n",
    "# 将字典转换为JSON格式的字符串\n",
    "t1 = json.dumps(dict1, ensure_ascii=False)\n",
    "print(t1)\n",
    "# 将JSON格式的字符串转换为字典\n",
    "t2=json.loads(t1)\n",
    "print(t2)"
   ]
  }
